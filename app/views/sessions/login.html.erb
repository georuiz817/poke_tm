<div class='welcome-page'>
<%= render 'welcome/form' %>
   <h2>Login</h2>
   <%= form_for @user, url: login_path do |f| %>
      <ul>
         <li>
            <%= f.label :email %>
            <%= f.email_field :email %>
         </li>
         <li>
            <%= f.label :password%>
            <%= f.password_field :password %>
         </li>
         <%= f.submit 'login' %>
      </ul>
   <% end %> 
</div>

